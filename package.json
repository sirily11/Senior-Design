  "scripts": {
    "add-locale": "lingui add-locale",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "start": "react-scripts start",
    "build": "react-scripts build && tsc public/electron.ts",
    "test": "react-scripts test --setupFiles ./src/setupTests.js",
    "eject": "react-scripts eject",
    "electron-pack": "build",
    "predist": "build",
    "win": "electron-builder -w",
    "mac": "electron-builder -m",
    "all": "electron-builder -mw",
    "clean": "rm -rf dist",
    "build-doc": "tsc src/components/utils/docs-generator/docs_generator.ts && node src/components/utils/docs-generator/docs_generator.js",
    "electron-dev-win": " tsc public/electron.ts && concurrently \"yarn start \" \"wait-on http://localhost:3000  && electron .\"",
    "electron-dev": " tsc public/electron.ts && concurrently \"BROWSER=none yarn start \" \"wait-on http://localhost:3000  && electron .\""
  },
  "build": {
    "appId": "app.test",
    "mac": {
      "category": "admin"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "mac": {
    "category": "blog.app.editor.production"
  },
  "main": "public/electron.js",
  "devDependencies": {
    "@testing-library/jest-dom": "^4.1.0",
    "concurrently": "^4.1.0",
    "electron": "^7.2.4",
